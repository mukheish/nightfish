#include "stdafx.h"
#include <iostream>
#include <cstdlib>
using namespace std;
#define n 6

int main()
{
	int i;
	double largest, ranNumbers[n];
	cout << "6 random generated numbers are: " << endl;
	for(i=0;i<n;i++)
	{
		ranNumbers[i] = rand();
		cout << ranNumbers[i] << "";
	}
	largest = ranNumbers[0];
	for (i = 1; i < n; i++)
		if (largest < ranNumbers[i])
			largest = ranNumbers[i];
	cout << largest << endl;
    return 0;
}
